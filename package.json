{
  "name": "jsonforms-tailwind-renderers",
  "version": "1.0.0",
  "description": "Modern JSONForms renderers built with React and Tailwind CSS",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npm run dev",
    "build": "rollup -c",
