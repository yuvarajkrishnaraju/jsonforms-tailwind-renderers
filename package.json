    "@rollup/plugin-typescript": "^11.0.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/lodash": "^4.17.20",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "autoprefixer": "^10.4.21",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.4",
